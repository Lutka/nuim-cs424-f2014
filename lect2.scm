;;; Use Scheme in a
;;; Read-eval-print loop (REPL)

(define hypot
  (lambda (x y)
    (sqrt (+ (* x x) (expt y 2)))))

;;; Bind two local variables at once!
(define test
  (lambda (a)
    ((lambda (b c)
       (+ (* b b) (* c c c)))
     (sin a)
     (cos a))))

;;; See what order they're computed in
(define test1
  (lambda (a)
    ((lambda (b c)
       (+ (* b b) (* c c c)))
     ((lambda (ignored) (sin a)) (print 1))
     ((lambda (ignored) (cos a)) (print 2)))))

;;; Okay
(define test2
  (lambda (a)
    (let ((b (sin a))
	  (c (cos a)))
      (+ (* b b) (* c c c)))))

;;; BAD!
(define test2b
  (lambda (a)
    (let ((b (sin a))
	  (c (cos b)))
      (+ (* b b) (* c c c)))))

;;; Okay
(define test2c
  (lambda (a)
    (let* ((b (sin a))
	   (c (cos b)))
      (+ (* b b) (* c c c)))))

;;; Higher-order because it takes a function
(define do-twice
  (lambda (f x)
    (f (f x))))

;;; Higher-order because it returns a function
(define add-n
  (lambda (n)
    (lambda (x)
      (+ x n))))

(define comp
  (lambda (f g)
    (lambda (x)
      (f (g x)))))

;;; Church coded pairs

;;; constructor
(define kons
  (lambda (a d)
    (lambda (z)
      (z a d))))

;;; accessors
(define kar
  (lambda (p)
    (p (lambda (a d) a))))

(define kdr
  (lambda (p)
    (p (lambda (a d) d))))

;;; manipulate one
(define add-konstree-leaves
  (lambda (kt)
    (if (number? kt)
	kt
	(+ (add-konstree-leaves (kar kt))
	   (add-konstree-leaves (kdr kt))))))

(define kt5
  (kons (kons 1 2) (kons (kons 3 4) 5)))

